    <script>
    $(document).ready(function () {

     

    var sizeoriginaltexto1 = $(".texto1").css('font-size');
    var sizeoriginaltexto1_submenu = $(".texto1_submenu").css('font-size');
    var sizeoriginalsizetexto1rodape = $(".texto1_rodape").css('font-size');
        // aumentando a fonte menu
    $(".inc-font").click(function () {
        var size = $(".texto1").css('font-size');

    size = size.replace('px', '');
    size = parseInt(size) + 1.4;
    $(".texto1").animate({ 'font-size': size + 'px' });

    //submenu

    var size2 = $(".texto1_submenu").css('font-size');

    size2= size2.replace('px', '');
    size2 = parseInt(size2) + 1.4;

    $(".texto1_submenu").animate({ 'font-size': size2 + 'px' });

    //rodape texto1_rodape

    var sizetexto1rodape = $(".texto1_rodape").css('font-size');

    sizetexto1rodape = sizetexto1rodape.replace('px', '');
    sizetexto1rodape = parseInt(sizetexto1rodape) + 1.4;

    $(".texto1_rodape").animate({ 'font-size': sizetexto1rodape + 'px' });

    });


    //diminuindo a fonte -------------------------
    $(".dec-font").click(function () {


        var size = $(".texto1").css('font-size');

        size = size.replace('px', '');
        size = parseInt(size) - 1.4;
        $(".texto1").animate({ 'font-size': size + 'px' });

        //submenu

        var size2 = $(".texto1_submenu").css('font-size');

        size2 = size2.replace('px', '');
        size2 = parseInt(size2) - 1.4;

        $(".texto1_submenu").animate({ 'font-size': size2 + 'px' });

        //rodape texto1_rodape

        var sizetexto1rodape = $(".texto1_rodape").css('font-size');

        sizetexto1rodape = sizetexto1rodape.replace('px', '');
        sizetexto1rodape = parseInt(sizetexto1rodape) - 1.4;

        $(".texto1_rodape").animate({ 'font-size': sizetexto1rodape + 'px' });

       

    });

    // resetando a fonte
    $(".res-font").click(function () {
        $(".texto1").animate({ 'font-size': sizeoriginaltexto1 });
        $(".texto1_submenu").animate({ 'font-size': sizeoriginaltexto1_submenu });
        $(".texto1_rodape").animate({ 'font-size': sizeoriginalsizetexto1rodape });
    });

    });
    </script>


