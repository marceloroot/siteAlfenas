                            <span class="sr-only">Next</span>
                        </a>
                    </div>

                </div>
            </div>
        </div>




        <!--Noticia-->
        <div class="row marginbottom15p margintop8">
            <div class="col-lg-12 col-md-12 col-xs-12 col-sm-12">

                <!--titulos da noticias-->
                <!--repeat-->
                @foreach (var variavel in ViewBag.Noticia)
                {
                    foreach (var listaimagem in variavel.ListImagem)
                    {
                        if (listaimagem.Destaque == true)
                        {
                            <div class="row">
                                <div class="col-lg-3 col-md-3 col-xs-12 col-sm-12">
                                    <hr class="linhahorizontalLateral " />

                                </div>
                            </div>
                            <div class="row">

                                <div class="col-lg-8 col-md-8 col-xs-12 col-sm-12 noticias">
                                    <a href="@Url.Action("Noticias", "Home", new { id = variavel.Id})">
                                        <h3>@variavel.Titulo</h3>
                                        <p>@variavel.Titulo</p>
                                    </a>
                                    <p>
                                        <span>por Ascom</span>  Ultima Modificação em @variavel.DataPublicacao.ToShortDateString() |  @variavel.DataPublicacao.Hour.ToString()H @variavel.DataPublicacao.Minute.ToString()min  |
                                        <img src="~/images/Inicio/redesociais/F1.png" alt="Facebook" title="Facebook" />
                                        <img src="~/images/Inicio/redesociais/T2.png" alt="Tiwitter" title="Tiwitter" />
                                        <img src="~/images/Inicio/redesociais/g3.png" alt="Google +" title="Google +" />
                                        <img src="~/images/Inicio/redesociais/i4.png" alt="i" +" title="i" />


                                    </p>

                                </div>
                                <div class="col-lg-4 col-md-4 col-xs-12 col-sm-12 imgNoticia">
                                    <a href="@Url.Action("Noticias", "Home", new { id = variavel.Id})">
                                        <img src="@baseurl/Uploads/Img/@listaimagem.Caminho" alt="@variavel.Titulo" title="@variavel.Titulo" class="img-responsive" />
                                    </a>
                                </div>




                            </div>
                            test++;
                        }
                    }
                }



            </div>
        </div>
    </div>



