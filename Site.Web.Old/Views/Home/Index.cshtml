                        <li data-target="#myCarousel2" data-slide-to="1"></li>
                        <li data-target="#myCarousel2" data-slide-to="2"></li>
                    </ol>

                    <!-- Wrapper for slides -->
                    <div class="carousel-inner imgbannerPrincipal">

                        @foreach (var variavel in ViewBag.banner)
                        {



                            foreach (var listaimagem in variavel.ListImagem)
                            {
                                if (listaimagem.Destaque == true)
                                {
                                    if (countBanner2 == 0)
                                    {
                                        <div class="item active">
                                            <a href="@Url.Action("Noticias", "Home", new { id = variavel.Id})">
                                                <img src="@baseurl/Uploads/Img/@listaimagem.Caminho" alt="@variavel.Titulo" title="@variavel.Titulo" />
                                                <div class="carousel-caption">
                                                    <h3>@variavel.Titulo</h3>
                                                    <p>@variavel.Titulo</p>
                                                </div>
                                            </a>
                                        </div>
                                        countBanner2++;
                                    }
                                    else
                                    {
                                        <div class="item">
                                            <a href="@Url.Action("Noticias", "Home", new { id = variavel.Id})">
                                                <img src="@baseurl/Uploads/Img/@listaimagem.Caminho" alt="@variavel.Titulo" title="@variavel.Titulo" />
                                                <div class="carousel-caption">
                                                    <h3>@variavel.Titulo</h3>
                                                    <p>@variavel.Titulo</p>
                                                </div>
                                            </a>
                                        </div>

                                    }


                                    break;
                                }
                            }



                        }



                        <!-- Left and right controls -->
                        <a class="left carousel-control" href="#myCarousel2" data-slide="prev">
                            <span class="glyphicon glyphicon-chevron-left"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="right carousel-control" href="#myCarousel2" data-slide="next">
